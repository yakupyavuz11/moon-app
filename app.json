{
  "expo": {
    "name": "purpy",
    "slug": "purpy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A5AE0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_icon.png",
        "backgroundColor": "#6A5AE0"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ],
    "experiments": {
      "tsconfigPaths": false
    }
  }
}
